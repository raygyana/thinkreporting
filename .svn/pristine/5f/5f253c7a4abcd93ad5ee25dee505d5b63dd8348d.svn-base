<section class="container-fluid my-3 custRow ">
  <div class="row ">
    <div class="col-md-12">
      <div class="whiteBg tabContent">
        <div class="row">
          <div class="col-12">
            <h4>Process</h4>
            <hr>
            <div class="border m-1 p-2 pt-4 mt-4">
              <fieldset>
                <legend class="text-uppercase">Process Setup</legend>
                <div class="row justify-content-between">
                  <div class="col-3">
                    <div class="form-group">
                      <div class="form-check form-check-inline ">
                        <label class="custom-control custom-checkbox ">
                          <input type="checkbox" (change)="setActiveFilterModel()" class="custom-control-input" checked>

                          <span class="  custom-control-label">Active </span>
                        </label>
                      </div>
                      <div class="form-check form-check-inline ">
                        <label class="custom-control custom-checkbox ">
                          <input id="inactivecheck" type="checkbox" (change)="setInActiveFilterModel()" class="custom-control-input" checked>

                          <span class="  custom-control-label">Inactive </span>
                        </label>
                      </div>

                    </div>

                  </div>


                </div>
              </fieldset>
            </div>
          </div>
        </div>

        <div class="agGridSize">
          <app-ag-grid [name]="'counterReports22'" [myGridOptions]="gridOptions" [rowSelection]="rowSelection" (myGridReady)="doOnGridReadyProductReports($event)">
          </app-ag-grid>
        </div>

      </div>
      <div class=" text-right mt-3">
        <!-- <button type="button" class="btn btn-primary" (click)="showSubmitJobPopUp()">Submit Job</button> -->
        <!-- <a [routerLink]="['/pages/review_job_istory']" class="btn btn-primary" [class.disabled]="rowselected ? false : true">Review Job History</a> -->
        <a [routerLink]="['/pages/review_job_istory']" class="btn btn-primary" >Review Job History</a>
        <!-- <a href="javascript:;" class="btn btn-primary" (click)="showNewProcessPopUp(emailNotibasicSetting)" [class.disabled]="rowselected ? false : true">Email Notification Setup</a> -->
        <a href="javascript:;" class="btn btn-primary" (click)="showNewProcessPopUp(emailNotibasicSetting)">Email Notification Setup</a>
      </div>
    </div>
  </div>
</section>
<app-delete-pop></app-delete-pop>


<app-custom-modal-pop-up [AlertType]="AlertType" [basicSetting]="emailNotibasicSetting" [template]="EmailNotificationSetUpTemplate"></app-custom-modal-pop-up>

<ng-template #EmailNotificationSetUpTemplate>
  <div class="content-box-inner">

    <div class="row justify-content-between">
      <div class="col-md-12">
        <div class="form-group">
          <label for="user">Email Address for Notification of job Completion</label>
          <input type="text" class="form-control" placeholder="Email" readonly>
        </div>
      </div>
    </div>
    <div class="row justify-content-between">
      <div class="col-12">
        <div class="form-group">
          <div class="form-check form-check-inline ">
            <label class="custom-control custom-radio ">
              <input type="radio" value="selected" name="emailnotif" (click)="setRadioValue($event)" class="custom-control-input">

              <span class="  custom-control-label">Selected Process Only</span>
            </label>
          </div>
          <div class="form-check form-check-inline ">
            <label class="custom-control custom-radio ">
              <input type="radio" name="emailnotif" (click)="setRadioValue($event)" value="All" class="custom-control-input">

              <span class="  custom-control-label">All Exiting Process of Type</span>
            </label>
          </div>
          <div class="form-check form-check-inline ">
            <label class="custom-control custom-radio ">
              <input type="radio" name="emailnotif" (click)="setRadioValue($event)" value="Default" class="custom-control-input" checked>

              <span class="  custom-control-label">System-wide Defalut </span>
            </label>
          </div>
        </div>
      </div>
    </div>
    <div class="form-row">
      <label for="inputPassword" class="col-md-3 form-group col-form-label">Main Purpose</label>

      <div *ngIf="showBox=='All'" class="col-md-9">
        <select class="form-control custom-select" id="sel1" (change)="myChangeHandler($event.target.value)">
          <option>Select</option>
          <option value="pages/auto-renewal_notify">
            Auto-Renewal Notify
          </option>
          <option value="pages/audit_paragraph_report">Audit Paragraph Report</option>
          <option value="pages/audit_galley">Audit Galley</option>
          <option value="pages/backlebel">Backlabel</option>
          <option value="pages/billing">Billing</option>
          <option value="pages/bacs">BACS DD</option>
          <option value="pages/cleanup">Cleanup</option>
          <option value="pages/installment_notices">Installment Notices</option>
          <option value="pages/lebel_estimate">Label Estimate</option>
          <option value="pages/list_extract">List Extract</option>
          <option value="pages/on_off">On/Off</option>
          <option value="pages/refund">Refund</option>
          <option value="pages/product_fulfillment">Product Fulfillment</option>
          <option value="pages/promotion">Promotion</option>
          <option value="pages/renewal">Renewal</option>
          <option value="pages/start/stop">Start/Stop</option>
          <option value="pages/payment">Payment</option>
          <option value="pages/bacs_billing">BACS Billing</option>
          <option value="pages/ics_payment_batch_listener">ICS Payment Batch Listener</option>
          <option value="pages/accounting_reconciliation">Accounting Reconciliation</option>
          <option value="pages/mass_kill">Mass Kill</option>
          <option value="pages/label">Label</option>
          <option value="pages/customer_deposit_reconciliation">Customer Deposit Reconciliation</option>
          <option value="pages/bacs_auddis">BACS AUDDIS</option>
          <option value="pages/credit_card_expiry_notify">Credit Card Expiry Notify</option>
          <option value="pages/credit_card_token_refresh">Credit Card Token Refresh </option>
        </select>
      </div>

      <div *ngIf="showBox=='selected'" class="col-md-9">
        <div class="form-group">
          <label for="user">Email Address for Notification of job Completion</label>
          <input type="text" class="form-control" placeholder="Email" readonly>
        </div>
      </div>

      <div *ngIf="showBox=='Default'" class="col-md-9">
        <div class="form-group">
          <input type="text" value="Renewal" class="form-control" placeholder="Email" readonly>
        </div>
      </div>

    </div>
    <div class="row">
      <div class="col-md-12 text-right my-3">
        <a (click)="okClick()" href="javascript:;" class="btn btn-primary">
          Ok
        </a>
        <button type="button" class="btn btn-light" data-dismiss="modal">
          Cancel
        </button>
      </div>
    </div>
  </div>
</ng-template>


<app-process-submit-job></app-process-submit-job>
