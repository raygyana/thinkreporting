<section class="container-fluid my-3 custRow ">
  <div class="row ">
    <div class="col-md-12">
      <div class="whiteBg tabContent">
        <div class="row">
          <div class="col-12">
            <h4>Review Job History</h4>
            <hr>
            <div class="row">
              <div class="col-6">
                <div class="border m-1 p-2 pt-4 mt-2">
                  <fieldset>
                    <legend class="text-uppercase">Job Filter</legend>
                    <div class="col-12">
                      <div class="form-check form-check-inline ">
                        <label class="custom-control custom-checkbox ">
                          <input type="checkbox" class="custom-control-input">

                          <span class="  custom-control-label">On Hold </span>
                        </label>
                      </div>

                      <div class="form-check form-check-inline ">
                        <label class="custom-control custom-checkbox ">
                          <input type="checkbox" class="custom-control-input">

                          <span class="  custom-control-label">Ready </span>
                        </label>
                      </div>

                      <div class="form-check form-check-inline ">
                        <label class="custom-control custom-checkbox ">
                          <input type="checkbox" class="custom-control-input">

                          <span class="  custom-control-label">Running</span>
                        </label>
                      </div>

                      <div class="form-check form-check-inline ">
                        <label class="custom-control custom-checkbox ">
                          <input type="checkbox" class="custom-control-input">

                          <span class="  custom-control-label">Day End</span>
                        </label>
                      </div>
                      <button (click)="showJobQueueChoosePopUp()" type="button" class="btn btn-primary">Queue..</button>
                    </div>
                  </fieldset>
                </div>
              </div>
              <div class="col-6 ">
                <div class=" float-right pt-2">


                  <button type="button" class="btn btn-primary" (click)="showResetDatePopUp()">Reset Start Date/Time</button>



                  <button type="button" class="btn btn-primary" disabled="">Restart Update</button>
                  <a [routerLink]="['/pages/job_log']" class="btn btn-primary">View Job Log</a>
                  <div class="pt-2">
                    <button type="button" class="btn btn-primary" disabled="">Manual Select</button>
                    <button type="button" class="btn btn-primary" disabled="">Manual Pay Response</button>
                    <!-- <button type="button" class="btn btn-primary">Delete</button>
                    <button type="button" class="btn btn-primary" disabled="">Reprint </button> -->
                    <button type="button" class="btn btn-primary" disabled="">Rollback</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>




        <div class="agGridSize">
          <app-ag-grid [name]="'counterReports'" (myGridReady)="doOnGridReadyCounterReports($event)">
          </app-ag-grid>
        </div>

        <!-- <table class="table table-striped table-responsive border mt-3">
          <thead>
            <tr>
              <th></th>
              <th>Job Id</th>
              <th>Description</th>
              <th>Note</th>
              <th>Status</th>
              <th>Step</th>
              <th>Priority</th>
              <th>Next Start</th>
              <th>Candidates</th>
              <th>Selected</th>
              <th>Updated</th>
              <th>Has Errors</th>
              <th>Process Id </th>
              <th>Drop Date</th>
              <th>Cutoff Date</th>

            </tr>
          </thead>
          <tbody>
            <tr>
              <td colspan="15" class="text-center">No Data Available</td>
            </tr>
          </tbody>
        </table> -->
      </div>

    </div>
  </div>
</section>



<app-custom-modal-pop-up [AlertType]="AlertType" [basicSetting]="chooseJobQueuebasicSetting" [template]="chooseJobQueueTemplate"></app-custom-modal-pop-up>
<app-custom-modal-pop-up [AlertType]="AlertType" [basicSetting]="restDatebasicSetting" [template]="restDateTemplate"></app-custom-modal-pop-up>

<ng-template #chooseJobQueueTemplate>
  <app-choose-job-queue></app-choose-job-queue>
</ng-template>

<ng-template #restDateTemplate>
  <button type="button" class="close crossIcon" data-dismiss="modal" aria-label="Close"> X
  </button>
  <div class="modal-body">
    <!-- <div class="row">
      <div class="col text-left">
        <h5 class="modal-title" id="exampleModalLabel">Reset Dates</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        </button>
      </div>
    </div>
    <hr> -->
    <div class="content-box-inner">
      <form>
        <div class="form-group row">
          <label for="inputPassword" class="col-sm-3 col-form-label text-right pr-0">Description</label>
          <div class="col-sm-9">
            <input type="text" class="form-control" placeholder="Customer Address Update">
          </div>
        </div>
        <div class="form-group row">
          <label for="inputPassword" class="col-sm-3 col-form-label text-right pr-0">Status</label>
          <div class="col-sm-9">
            <input type="text" class="form-control" placeholder="Ready Update">
          </div>
        </div>
        <div class="form-group row">
          <label for="inputPassword" class="col-sm-3 col-form-label text-right pr-0">Start Date/Time</label>
          <div class="col-sm-9">
            <input type="text" class="form-control" placeholder="">
          </div>
        </div>

        <div class="form-group row">
          <label for="inputPassword" class="col-sm-3 col-form-label text-right pr-0">Drop Time</label>
          <div class="col-sm-5 pr-0">
            <input type="datetime-local" class="form-control" placeholder="">
          </div>
          <div class="col-sm-4 text-right">
            <button type="button" class="btn btn-primary" data-dismiss="modal">
              Same as Start
            </button>
          </div>
        </div>

        <div class="form-group row">
          <label for="inputPassword" class="col-sm-3 col-form-label text-right pr-0">Cutoff D/T</label>
          <div class="col-sm-5 pr-0">
            <input type="text" class="form-control" placeholder="">
          </div>
          <div class="col-sm-4 text-right">
            <button type="button" class="btn btn-primary" data-dismiss="modal">
              Same as Start
            </button>
          </div>
        </div>

        <div class="form-group row">
          <label for="inputPassword" class="col-sm-3 col-form-label text-right pr-0">Priority</label>
          <div class="col-sm-3">
            <input type="text" class="form-control" placeholder="1">
          </div>

        </div>
      </form>
      <div class="row">
        <div class="col-md-12 text-right my-3">
          <button type="button" class="btn btn-primary">
            Ok
          </button>
          <button type="button" class="btn btn-light" data-dismiss="modal">
            Cancel
          </button>
        </div>
      </div>
    </div>
  </div>

</ng-template>
