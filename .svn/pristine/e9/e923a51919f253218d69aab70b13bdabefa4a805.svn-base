<div class="tab-content">
  <form #baseForm="ngForm">
    <div id="tab1" class="tab-pane fade show active ">
      <div class="whiteBg tabContent">
        <div class="content-box-inner">
          <div class="row justify-content-between align-items-center">
            <div class="col-md-3">
              <div class="form-group row">
                <label for="historyFilter" class="col-sm-5 pr-0 pl-0 col-form-label">Note Filters</label>
                <div class="col-sm-7">
                  <app-select name="test" [(ngModel)]="notesModel.noteFilter.value" (change)="onDropDownChange($event)" [Data]="ddOptionsNotesData">
                  </app-select>
                </div>
              </div>
            </div>
          </div>
          <!-- <div class="row"> -->
          <div class="col-md-12 text-right my-3">
            <button type="button" class="btn btn-primary" [disabled]="disabled" (click)="sendAddData()">
              <i class="fa fa-plus" aria-hidden="true"></i> Add Note</button>
          </div>
          <!-- </div> -->
          <hr class="borderBtm">
          <div class="row">
            <div class="col-md-12">
              <div class="agGridSize">
                <app-ag-grid [name]="'notesReports'" [myGridOptions]="gridOptionsReports" (myGridReady)="doOnGridReadyAutoRenewalReports($event)"></app-ag-grid>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </form>
</div>


<app-custom-modal-pop-up [basicSetting]="editNotePopupSettings" [template]="editNotesTemplate"></app-custom-modal-pop-up>
<ng-template #editNotesTemplate>
  <app-edit-note (Change)="UpdateDataOnEdit()" [editNote]="array"></app-edit-note>
</ng-template>


<app-custom-modal-pop-up [basicSetting]="addNotePopupSettings" [template]="addNotesTemplate"></app-custom-modal-pop-up>


<ng-template #addNotesTemplate>
  <app-add-note [reInitComponent]="reInitAddNote" (UpdateData)="UpdateDataOnEdit()" [addNoteData]="selectedData"></app-add-note>
</ng-template>
