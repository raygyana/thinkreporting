<div class="whiteBg tabContent">
  <div class="row">
    <div class="col-12 pt-3 pb-2">
      <div class="form-check form-check-inline pl-1">
        <label class="custom-control custom-checkbox">
          <input type="checkbox" class="custom-control-input">
            
          <span class="  custom-control-label">Renewal for Requalification</span>
        </label>
      </div>
      <div class="form-check form-check-inline">
        <label class="custom-control custom-checkbox">
          <input type="checkbox" class="custom-control-input">
            
          <span class="  custom-control-label">Generate Auto Renewal Offers</span>
        </label>
      </div>
      <div class="form-check form-check-inline">
        <label class="custom-control custom-checkbox">
          <input type="checkbox" class="custom-control-input">
            
          <span class="  custom-control-label">Generate Notices for Package Members</span>
        </label>
      </div>
    </div>


    <div class="col-12">
      <div class="border mt-4 p-2 pt-4">
        <fieldset>
          <legend class="text-uppercase">renewal Definition</legend>
          <div class="agGridSize">
            <app-ag-grid [name]="'RenewalDefinitionReports'" (myGridReady)="doOnGridReadyRenewalDefinitionReports($event)"></app-ag-grid>
          </div>
        </fieldset>
      </div>
      <div class="pt-4">
        <div class="border mt-4 p-2 pt-4">
          <fieldset>
            <legend class="text-uppercase">Applies to These Order Classes</legend>

            <div class="col-md-12">
              <div class="d-flex flex-row-reverse align-items-center ">

                <div class=" col-3 p-2 ">
                  <app-dropdown-with-description name="test" [(ngModel)]="dropDown" [basicSetting]="basicSetting2" (change)="setFilterModel1($event)">
                  </app-dropdown-with-description>

                </div>
                <div class="p-2 ">Show :</div>
              </div>
              <div class="clearfix "></div>
            </div>

            <div class="form-check form-check-inline">
              <label class="custom-control custom-checkbox">
                <input type="checkbox" class="custom-control-input">
                  
                <span class="  custom-control-label">Generate Renewal Codes</span>
              </label>
            </div>
            <div class="agGridSize">
              <app-ag-grid [name]="'RenewalOrderClassReports'" [myGridOptions]="gridOptionsReports1" (myGridReady)="doOnGridReadyRenewalOrderClassReports($event)"></app-ag-grid>
            </div>
          </fieldset>
        </div>

        <div class="pt-4">
          <div class="border mt-4 p-2 pt-4">
            <fieldset>
              <legend class="text-uppercase">Efforts</legend>
              <div class="agGridSize">
                <app-ag-grid [name]="'RenewalEffortsReports'" [myGridOptions]="gridOptionsReports" (myGridReady)="doOnGridReadyRenewalEffortsReports($event)"></app-ag-grid>
              </div>
            </fieldset>
          </div>
        </div>

        <div class="row">
          <div class="col-6 pt-3">
            <div class="border mt-4 p-2 pt-4">
              <fieldset>

                <div class="row">

                  <div class="col-md-4">
                    <div class="form-group ">
                      <label for="user">Renewal Card</label>
                      <app-dropdown-with-description name="test" [(ngModel)]="dropDown" [basicSetting]="basicSetting3">
                      </app-dropdown-with-description>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group ">
                      <label for="user">Nth Definition</label>
                      <app-dropdown-with-description name="test" [(ngModel)]="dropDown" [basicSetting]="basicSetting">
                      </app-dropdown-with-description>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group ">
                      <label for="user">Volume Group</label>
                      <input type="text" class="form-control" placeholder=" ">
                    </div>
                  </div>

                </div>
              </fieldset>
            </div>
          </div>
          <div class="col-6 pt-3">
            <div class="border mt-4 p-2 pt-4">
              <fieldset>
                <legend class="text-uppercase">Generate Group Bar Codes</legend>
                <div class="row">
                  <div class="col-md-4">
                    <div class="form-check ">
                      <label class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input ">
                        <span class="custom-control-indicator disabled"></span>
                        <span class="  custom-control-label disabled">renew-to </span>
                      </label>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-check">
                      <label class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input ">
                        <span class="custom-control-indicator disabled"></span>
                        <span class="  custom-control-label disabled">Renewal Definition</span>
                      </label>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-check ">
                      <label class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input ">
                        <span class="custom-control-indicator disabled"></span>
                        <span class="  custom-control-label disabled">Currency</span>
                      </label>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-check ">
                      <label class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input ">
                        <span class="custom-control-indicator disabled"></span>
                        <span class="  custom-control-label disabled">Order Class</span>
                      </label>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-check ">
                      <label class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input ">
                        <span class="custom-control-indicator disabled"></span>
                        <span class="  custom-control-label disabled">Order Number</span>
                      </label>
                    </div>
                  </div>

                </div>
              </fieldset>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
