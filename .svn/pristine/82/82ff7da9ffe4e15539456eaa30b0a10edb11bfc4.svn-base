<div class="tab-content">
  <div id="tab1" class="tab-pane fade show active ">
    <div class="whiteBg tabContent">
      <div class="content-box-inner">
        <div class="row justify-content-between align-items-center">
          <div class="col-md-4">
            <div class="form-group row">
              <label for="historyFilter" class="col-sm-5 pr-0 pl-0 col-form-label">Attachment Filters</label>
              <div class="col-sm-7">
                <app-select [Data]="ddOptionsattachmentListData" [(ngModel)]="attachmentModel.attachmentFilter.value" name="attachmentFilter"
                  (change)="operaterChange($event)">
                </app-select>
              </div>
            </div>
          </div>
        </div>
        <hr class="borderBtm">
        <div class="row">
          <div class="col-md-12">
            <div class="agGridSize">
              <app-ag-grid [name]="'attachReports'" [myGridOptions]="gridOptionsReports" (myGridReady)="doOnGridReadyAttachReports($event)"></app-ag-grid>
            </div>
          </div>
        </div>
      </div>
      <div class="row pt-3 pb-5">
        <div class="col-12 text-right">
          <button id="addAttachment" class="btn btn-primary" data-toggle="modal" (click)="openAttachment($event)" [disabled]="!isDisabled">
            <i class="fa fa-plus" aria-hidden="true"></i> Add New..</button>
        </div>
      </div>
    </div>
  </div>
</div>
<app-custom-modal-pop-up [basicSetting]="addnewpopupSetting" [template]="addAttachmentTemplate"></app-custom-modal-pop-up>
<ng-template #addAttachmentTemplate>
  <app-add-attachment></app-add-attachment>
</ng-template>
