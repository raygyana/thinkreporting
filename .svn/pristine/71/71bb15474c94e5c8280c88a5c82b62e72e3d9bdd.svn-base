<app-custom-modal-pop-up [AlertType]="AlertType" [basicSetting]="deletePopUpbasicSetting" [template]="DeleteTemplate"></app-custom-modal-pop-up>

<ng-template #DeleteTemplate>

  <div class="content-box-inner">
    <div class="form-row">
      <p>Do you want to Delete '
        <b>{{massage}}'</b> ?</p>
    </div>
    <div class="row">
      <div class="col-md-12 text-right my-3">
        <a (click)="deleteClick()" class="btn btn-primary">
          Ok
        </a>
        <button type="button" class="btn btn-light" data-dismiss="modal">
          Cancel
        </button>
      </div>
    </div>
  </div>
</ng-template>
