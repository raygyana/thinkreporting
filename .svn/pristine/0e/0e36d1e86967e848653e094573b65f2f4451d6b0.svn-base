    <div class="form-row">
      <div class="col-md-6">
        <div class="form-group">
          <div class="row">
            <div class="col-md-3">
              <label for="user">Decription</label>
            </div>
            <div class="col-md-9">
              <input type="text" class="form-control">
            </div>
          </div>
        </div>
      </div>

      <div class="col-md-6">
        <div class="form-group">
          <div class="row">
            <div class="col-md-5">
              <label for="user">Default Job Queue</label>
            </div>
            <div class="col-md-7">
              <input type="text" value="" class="form-control">
            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="form-row">
      <div class="col-md-6">
        <div class="form-group">
          <div class="row">
            <div class="col-md-3">
              <label for="user">Job Note</label>
            </div>
            <div class="col-md-9">
              <input type="text" class="form-control">
            </div>
          </div>
        </div>
      </div>

      <div class="col-md-6">
        <div class="form-check form-check-inline ">
          <label class="custom-control custom-checkbox ">
            <input type="checkbox" name="emailnotif" (click)="setRadioValue($event)" value="All" class="custom-control-input">

            <span class="  custom-control-label">Send back issue for reinstated orders </span>
          </label>
        </div>
      </div>
    </div>

    <div class="form-row">

      <div class="form-group">
        <div class="row">
          <div class="col-md-6">
            <div class="row">
              <div class="col-md-4">
                <label for="user">Job Priority</label>
              </div>
              <div class="col-md-2">
                <input type="text" class="form-control" placeholder="1">
              </div>
              <div class="col-md-6">
                <div class="form-check form-check-inline ">
                  <label class="custom-control custom-checkbox ">
                    <input type="checkbox" name="emailnotif" (click)="setRadioValue($event)" value="All" class="custom-control-input">

                    <span class="  custom-control-label">Hold Job</span>
                  </label>
                </div>
              </div>


            </div>
          </div>
          <div class="col-md-6">
            <div class="row">
              <div class="col-md-6">
                <div class="form-check form-check-inline ">
                  <label class="custom-control custom-checkbox ">
                    <input type="checkbox" name="emailnotif" (click)="setRadioValue($event)" value="All" class="custom-control-input">

                    <span class="  custom-control-label">Hold Output </span>
                  </label>
                </div>
              </div>
              <div class="col-md-6">
                <div class="form-check form-check-inline ">
                  <label class="custom-control custom-checkbox ">
                    <input type="checkbox" name="emailnotif" (click)="setRadioValue($event)" value="All" class="custom-control-input">

                    <span class="  custom-control-label">Hold Update </span>
                  </label>
                </div>
              </div>
            </div>
          </div>

        </div>
      </div>
    </div>

    <div class="form-row">

      <div class="form-group">
        <div class="row">
          <div class="col-md-3">
            <label for="user">Text File Logging Options</label>
          </div>
          <div class="col-md-1.3">
            <div class="form-check form-check-inline ">
              <label class="custom-control custom-radio ">
                <input type="radio" name="logginOption" (click)="setRadioValue($event)" value="All" class="custom-control-input">

                <span class="  custom-control-label">None</span>
              </label>
            </div>
          </div>

          <div class="col-md-1.3">
            <div class="form-check form-check-inline ">
              <label class="custom-control custom-radio ">
                <input type="radio" name="logginOption" (click)="setRadioValue($event)" value="All" class="custom-control-input">

                <span class="  custom-control-label">Normal </span>
              </label>
            </div>
          </div>

          <div class="col-md-1.3">
            <div class="form-check form-check-inline ">
              <label class="custom-control custom-radio ">
                <input type="radio" name="logginOption" (click)="setRadioValue($event)" value="All" class="custom-control-input">

                <span class="  custom-control-label">detailed </span>
              </label>
            </div>
          </div>

          <div class="col-md-4">
            <div class="form-group">
              <div class="row">
                <div class="col-md-4">
                  <label for="user">Start Date/Time</label>
                </div>
                <div class="col-md-8">
                  <input type="text" class="form-control" placeholder="">
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>


    <div class="form-row">
      <div class="col-md-4">
        <div class="form-group">
          <p>Select payment between dates: </p>
        </div>
      </div>
      <div class="col-md-4">
        <div class="form-group">
          <div class="row">
            <div class="col-md-3">
              <label for="user">Start Date</label>
            </div>
            <div class="col-md-9">
              <input type="text" class="form-control" placeholder="">

            </div>
          </div>
        </div>
      </div>

      <div class="col-md-4">
        <div class="form-group">
          <div class="row">
            <div class="col-md-3">
              <label for="user">End Date</label>
            </div>
            <div class="col-md-9">
              <input type="text" value="" class="form-control" placeholder="">
            </div>
          </div>
        </div>
      </div>
    </div>



    <div class="row">
      <div class="col-md-12 text-right my-3">
        <a (click)="okClick()" class="btn btn-primary">
          Ok
        </a>
        <button type="button" class="btn btn-light" data-dismiss="modal">
          Cancel
        </button>
      </div>
    </div>
  </div>
</ng-template>
