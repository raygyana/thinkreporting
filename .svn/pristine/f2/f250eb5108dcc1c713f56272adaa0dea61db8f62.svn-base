<section class="container-fluid my-3 custRow ">

  <section class="container-fluid my-3 custRow ">
    <my-tabs #staticTabs>
      <my-tab tabTitle="Description" [tabId]="BILLING_DESCRIPTION_REPORT">
        <div class="whiteBg tabContent">
          <form class="form-control" name="form" #f="ngForm" ngNativeValidate>
            <div class="border m-1 p-2 pt-4">
              <fieldset>
                <div class="row">
                  <div class="col-md-4">
                    <div class="form-group">
                      <label for="user">Job Description</label>
                      <input type="text" class="form-control" placeholder="Billing">
                    </div>
                  </div>
                  <!-- <div class="col-md-4">
                    <div class="form-group">
                      <label for="user">ID</label>
                      <input type="text" class="form-control" placeholder=" ">
                    </div>
                  </div> -->
                  <div class="col-md-4">
                    <div class="form-group">
                      <label for="user">Job Notes
                        <span class="mandatory">*</span>
                      </label>
                      <input class="form-control" [(ngModel)]="description1" #description name="description1" id="description" required>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group">
                      <label for="user">Default Job Queue</label>
                      <app-dropdown-with-description name="test" [(ngModel)]="dropDown" [basicSetting]="basicSetting">
                      </app-dropdown-with-description>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group">
                      <label for="user">Start Date/Time</label>
                      <input type="text" class="form-control" placeholder="">
                    </div>
                  </div>

                  <div class="col-md-4">
                    <div class="form-group">
                      <label for="user">Drop Date</label>
                      <app-dropdown-with-description name="test" [(ngModel)]="dropDown" [basicSetting]="basicSetting">
                      </app-dropdown-with-description>
                    </div>
                  </div>
                  <div class="col-md-4">
                    <div class="form-group">
                      <label for="user">Cutoff Date/Time</label>
                      <input type="text" class="form-control" placeholder="">
                    </div>
                  </div>


                  <div class="col-md-4">
                    <div class="form-group">
                      <label for="user">NTh-ing </label>
                      <app-dropdown-with-description name="test" [(ngModel)]="dropDown" [basicSetting]="basicSetting">
                      </app-dropdown-with-description>
                    </div>
                  </div>

                  <div class="col-12">

                    <div class="form-check form-check-inline">
                      <label class="custom-control custom-checkbox mr-0">
                        <input type="checkbox" class="custom-control-input" disabled="">
                          
                        <span class="  custom-control-label">Hold For Manual Select</span>
                      </label>
                    </div>


                    <div class="form-check form-check-inline">
                      <label class="custom-control custom-checkbox ">
                        <input type="checkbox" class="custom-control-input">
                          
                        <span class="  custom-control-label">Active</span>
                      </label>
                    </div>



                    <div class="form-check form-check-inline">
                      <label class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input" data-toggle="modal" data-target="#repeating">
                          
                        <span class="  custom-control-label">Repeating</span>
                      </label>
                    </div>
                  </div>

                </div>
              </fieldset>


            </div>

            <div class="row pt-3">
              <div class="col-12">
                <div class="border m-1 p-2 pt-4">
                  <fieldset>
                    <legend class="text-uppercase">Also Include </legend>
                    <div class="row">
                      <div class="col-md-3">
                        <div class="form-check form-check-inline">
                          <label class="custom-control custom-checkbox">
                            <input type="checkbox" class="custom-control-input" disabled="">
                              
                            <span class="  custom-control-label">Audit</span>
                          </label>
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div class="form-check form-check-inline">
                          <label class="custom-control custom-checkbox ">
                            <input type="checkbox" class="custom-control-input " disabled="">
                            <span class="custom-control-indicator "></span>
                            <span class="  custom-control-label ">Inserts</span>
                          </label>
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div class="form-check form-check-inline ">
                          <label class="custom-control custom-checkbox ">
                            <input type="checkbox" class="custom-control-input " disabled="">
                            <span class="custom-control-indicator "></span>
                            <span class="  custom-control-label ">Splits</span>
                          </label>
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div class="form-check form-check-inline ">
                          <label class="custom-control custom-checkbox ">
                            <input type="checkbox" class="custom-control-input " disabled="">
                            <span class="custom-control-indicator "></span>
                            <span class="  custom-control-label ">Renewals</span>
                          </label>
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div class="form-check form-check-inline ">
                          <label class="custom-control custom-checkbox ">
                            <input type="checkbox" class="custom-control-input " disabled="">
                            <span class="custom-control-indicator "></span>
                            <span class="  custom-control-label ">Billing</span>
                          </label>
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div class="form-check form-check-inline ">
                          <label class="custom-control custom-checkbox ">
                            <input type="checkbox" class="custom-control-input ">
                            <span class="custom-control-indicator "></span>
                            <span class="  custom-control-label ">Installment</span>
                          </label>
                        </div>
                      </div>
                      <div class="col-md-3">
                        <div class="form-check form-check-inline ">
                          <label class="custom-control custom-checkbox ">
                            <input type="checkbox" class="custom-control-input " disabled="">
                            <span class="custom-control-indicator "></span>
                            <span class="  custom-control-label ">Auto Renewals</span>
                          </label>
                        </div>
                      </div>

                    </div>
                  </fieldset>
                </div>
              </div>
            </div>
            <div class="row pt-3">
              <div class="col-12">
                <div class="border m-1 p-2 pt-4">
                  <fieldset>
                    <legend class="text-uppercase">Order Class (es) Included</legend>
                    <div class="row justify-content-between align-items-center ">
                      <div class="col-md-6 ">
                        <div class="d-flex flex-row align-items-center ">
                          <div class="p-2 ">Order Class</div>
                          <div class="col-3 p-2">
                            <app-dropdown-with-description name="test" [(ngModel)]="dropDown" [basicSetting]="basicSetting2" disabled>
                            </app-dropdown-with-description>
                          </div>
                          <div class="pt-3 ">
                            <div class="form-check form-check-inline">
                              <label class="custom-control custom-checkbox mr-0">
                                <input type="checkbox" class="custom-control-input">
                                  
                                <span class="  custom-control-label">Include All</span>
                              </label>
                            </div>
                          </div>
                        </div>
                      </div>
                      <div class="col-md-6 ">
                        <div class="d-flex flex-row-reverse align-items-center ">

                          <div class=" col-3 p-2 ">
                            <app-dropdown-with-description name="test" [(ngModel)]="dropDown" (change)="setFilterModel($event)" [basicSetting]="basicSetting1">
                            </app-dropdown-with-description>

                          </div>
                          <div class="p-2 ">Show :</div>
                        </div>
                        <div class="clearfix "></div>
                      </div>
                    </div>
                    <div class="agGridSize">
                      <app-ag-grid [name]="'BillingDescriptionReports'" [myGridOptions]="gridOptionsReports" (myGridReady)="doOnGridReadyBillingDescriptionReports($event)"></app-ag-grid>
                    </div>
                  </fieldset>

                </div>

              </div>
            </div>
            <div class="row pt-3">
              <div class="col-12">
                <div class="border m-1 p-2 pt-4">
                  <fieldset>
                    <legend class="text-uppercase">Label Format Attributes</legend>
                    <div class="row">
                      <div class="col-md-4">
                        <div class="form-group ">
                          <label for="user">Length</label>
                          <input type="text" class="form-control" placeholder="50">
                        </div>
                      </div>
                      <div class="col-md-4">
                        <div class="form-group ">
                          <label for="user">Group</label>
                          <app-dropdown-with-description name="test" [(ngModel)]="dropDown" [basicSetting]="basicSetting">
                          </app-dropdown-with-description>
                        </div>
                      </div>
                      <div class="col-md-4">
                        <div class="form-group ">
                          <label for="user">Keyline</label>
                          <app-dropdown-with-description name="test" [(ngModel)]="dropDown" [basicSetting]="basicSetting">
                          </app-dropdown-with-description>
                        </div>
                      </div>
                    </div>
                  </fieldset>
                </div>
              </div>
            </div>

            <div class="row pt-3">
              <div class="col-6">
                <div class="row">
                  <div class="col-md-5">
                    <div class="border m-1 p-2 pt-4">
                      <fieldset>
                        <legend class="text-uppercase">Gracing</legend>
                        <div class="form-check ">
                          <label class="custom-control custom-checkbox ">
                            <input type="checkbox" class="custom-control-input ">
                              
                            <span class="  custom-control-label">Gracing New Expires</span>
                          </label>
                        </div>

                        <div class="form-check">
                          <label class="custom-control custom-checkbox ">
                            <input type="checkbox" class="custom-control-input ">
                              
                            <span class="  custom-control-label">Serve Current Graces</span>
                          </label>
                        </div>
                      </fieldset>
                    </div>
                  </div>
                  <div class=" col-md-7 pt-4 mt-2">

                    <div class="row">
                      <div class="col-6 p-0 pr-2">
                        <div class="form-group ">
                          <label for="user">Bundle Threshold</label>
                          <input type="text" class="form-control" placeholder=" ">
                        </div>
                      </div>
                      <div class="col-6 p-0 pl-2">
                        <div class="form-group ">
                          <label for="user">DD Days Passed for Pulls</label>
                          <input type="text" class="form-control" placeholder=" " disabled="">
                        </div>
                      </div>
                    </div>

                  </div>
                </div>


              </div>

              <div class="col-6">
                <div class="border m-1 p-2 pt-4">
                  <fieldset>
                    <legend class="text-uppercase">Inventory</legend>
                    <div class="row">

                      <div class="col-md-4">
                        <div class="form-group ">
                          <label for="user">Priority Sort</label>
                          <input type="text" class="form-control" placeholder=" " disabled="">
                        </div>
                      </div>
                      <div class="col-md-4">
                        <div class="form-group ">
                          <label for="user"></label>
                          <input type="text" class="form-control" placeholder=" " disabled="">
                        </div>
                      </div>

                      <div class="col-md-4 pt-4">
                        <div class="form-check pt-2">
                          <label class="custom-control custom-checkbox">
                            <input type="checkbox" class="custom-control-input " disabled="">
                            <span class="custom-control-indicator disabled"></span>
                            <span class="  custom-control-label disabled">Picklist</span>
                          </label>
                        </div>
                      </div>
                    </div>
                  </fieldset>
                </div>
              </div>
            </div>

          </form>
          <app-process-edit-footer [form]="f" (onDoSave)=doOnSave($event)></app-process-edit-footer>
        </div>

      </my-tab>

      <my-tab tabTitle="Output" [tabId]="BILLING_OUTPUT_REPORT">
        <app-output></app-output>
      </my-tab>

      <my-tab tabTitle="Billing" [tabId]="BILLING_REPORT">
        <app-billing></app-billing>
      </my-tab>

      <my-tab tabTitle="Renewals" [tabId]="RENEWALS_REPORT">
        <app-auto-renewal-notify></app-auto-renewal-notify>
      </my-tab>
    </my-tabs>
  </section>

</section>
