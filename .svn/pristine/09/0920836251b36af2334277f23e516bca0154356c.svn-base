<div class="modal-body">

  <div class="clearfix"></div>
  <form action="addOrder" method="POST">
    <div class="content-box-inner">
      <div class="group ">
        <div class="">
          <div class="row ">
            <div class="form-group col-md-4 ">
              <label for=" ">Order Number</label>
              <input type="text" class="form-control " readonly="readonly" [(ngModel)]="orderInProgressModel.orderhdrId.value" name="orderhdrId">
            </div>
            <div class="form-group col-md-4 ">
              <label for=" ">Date</label>
              <input type="text" class="form-control " readonly="readonly" name='orderdate' [(ngModel)]="orderInProgressModel.orderdate.value">
            </div>
            <div class="form-group col-md-4 ">
              <label for=" ">PO Number</label>
              <input name="poNumber" class="form-control " type="text" [(ngModel)]="orderInProgressModel.poNumber.value">
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="clearfix"></div>

    <div class="row ">
      <div class="col-12">
        <div class="agGridSize">
          <!-- <div class="dataTables_wrapper no-footer">
          <div class="agGridSize-single-row"> -->
          <app-ag-grid [name]="'documentreference'" [myGridOptions]="gridOptions" (myGridReady)="doOnGridReadyDocRef($event)" [extraOption]="customExtraOption"
            [export]="false">
          </app-ag-grid>
          <!-- </div>
        </div> -->
        </div>
        <!-- 
        <app-ag-grid [name]="'documentreference'" [myGridOptions]="gridOptions" (myGridReady)="doOnGridReadyDocRef($event)" [export]="false" [extraOption]="customExtraOption">
        </app-ag-grid> -->


      </div>
    </div>

    <div class="clearfix"></div>
    <div class="content-box-inner">
      <div class="row">
        <div class=" col-sm-12 col-md-5   mt-3">
          <div class="border mt-2  p-2 pt-4  mt-sm-4 ">
            <fieldset>
              <legend class=" text-uppercase">Warnings</legend>
              <div class="row">
                <div class=" col-sm-12">
                  <textarea class="form-control" style="resize:none;height:160px;" disabled=""></textarea>
                </div>
              </div>
            </fieldset>
          </div>
        </div>

        <div class=" col-sm-7  mt-3">
          <div class="border mt-2  p-2 pt-4  mt-sm-4 ">
            <fieldset>
              <legend class=" text-uppercase">Order Totals</legend>
              <div class="row">
                <div class=" col-sm-12">
                  <div class="form-row">
                    <div class="form-group col-sm-6 col-md-6 col-lg-6 ">
                      <label for="inputCity" class="col-form-label ">Order Item Type</label>
                    </div>
                    <div class=" col-sm-6 col-md-6 col-lg-6 ">
                      <select class="form-control custom-select">
                        <option value="">All</option>
                        <option value="">Subscription</option>
                      </select>
                    </div>
                  </div>

                  <div class="form-row">
                    <div class="form-group col-sm-6 col-md-6 col-lg-6 ">
                      <label for="inputCity" class="col-form-label ">Incl Break Type Amts</label>
                    </div>
                    <div class=" col-sm-6 col-md-6 col-lg-6 ">
                      <div class="form-row">
                        <div class=" col-sm-6 col-md-6 col-lg-6 ">
                          <input type="checkbox" name="chkItem" (click)="itemCheck(chkItem?false:true)" [(ngModel)]="chkItem">
                          <label for="itemId"> Item</label>
                        </div>
                        <div class=" col-sm-6 col-md-6 col-lg-6 ">
                          <input type="checkbox" [(ngModel)]="chkDel" name="chkDel" (click)="delCheck(chkDel?false:true)">
                          <label for="DelId"> Del</label>
                        </div>
                      </div>
                      <div class="form-row">
                        <div class=" col-sm-6 col-md-6 col-lg-6 ">

                          <input type="checkbox" [(ngModel)]="chkTax" name="chkTax" (click)="taxCheck(chkTax?false:true)">
                          <label for="taxId"> Tax</label>
                        </div>
                        <div class=" col-sm-6 col-md-6 col-lg-6 ">
                          <input type="checkbox" [(ngModel)]="chkCom" name="chkCom" (click)="comCheck(chkCom?false:true)">
                          <label for="comId"> Com</label>
                        </div>
                      </div>
                    </div>
                  </div>

                  <div class="form-row mt-2">
                    <div class="form-group col-sm-6 col-md-6 col-lg-6">
                      <label for="inputCity" class="col-form-label ">Base Amt </label>
                    </div>
                    <div class=" col-sm-6 col-md-6 col-lg-6 ">


                      <input type="text" readonly="readonly" class="form-control" id="baseAmtId" [(ngModel)]="totalbaseAmt" name="totalbaseAmt">

                    </div>
                  </div>

                </div>
              </div>
            </fieldset>
          </div>
        </div>

      </div>
    </div>
    <p class="text-right mt-2">
      <button type="submit" class="btn btn-primary" data-dismiss="modal" (click)="addItm()">
        <i class="fa fa-plus "></i> Add Item
      </button>
      <button type="submit" class="btn btn-primary" data-dismiss="modal" (click)="orderSave()">Save Order</button>
    </p>

  </form>
</div>
