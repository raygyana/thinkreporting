<section class="container-fluid my-3 custRow ">
  <my-tabs #staticTabs>
    <my-tab tabTitle="Description" [tabId]="REFUND_DESCRIPTION_REPORT">
      <div class="whiteBg tabContent">


        <fieldset>


          <div class="row">
            <div class="col-md-4">
              <div class="form-group">
                <label for="user">Main Purpose</label>
                <input type="text" class="form-control" placeholder="Refund">
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <label for="user">ID</label>
                <input type="text" class="form-control" placeholder=" ">
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <label for="user">Description</label>
                <input type="text" class="form-control" placeholder=" ">
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group ">
                <label for="user">Default Priority</label>
                <input type="text" class="form-control" placeholder=" ">
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group ">
                <label for="user">Extract Filter</label>
                <select class="form-control custom-select">
                  <option>0</option>
                  <option>0</option>
                </select>
              </div>
            </div>
            <div class="col-md-4">
              <div class="form-group">
                <label for="user">Default Queue</label>
                <select class="form-control custom-select">
                  <option>auto</option>
                  <option></option>

                </select>
              </div>
            </div>
          </div>

          <div class="form-check form-check-inline">
            <label class="custom-control custom-checkbox">
              <input type="checkbox" class="custom-control-input" disabled="">
                
              <span class="  custom-control-label">Run credit card process during process</span>
            </label>
          </div>

          <div class="form-check form-check-inline">
            <label class="custom-control custom-checkbox mr-0">
              <input type="checkbox" class="custom-control-input">
                
              <span class="  custom-control-label">Process Deposit Account Payments</span>
            </label>
          </div>

          <div class="form-check form-check-inline">
            <label class="custom-control custom-checkbox ">
              <input type="checkbox" class="custom-control-input">
                
              <span class="  custom-control-label">Active</span>
            </label>
          </div>

          <div class="form-check form-check-inline">
            <label class="custom-control custom-checkbox">
              <input type="checkbox" class="custom-control-input" data-toggle="modal" data-target="#repeating" disabled="">
                
              <span class="  custom-control-label">Repeating</span>
            </label>
          </div>

        </fieldset>





        <div class="row">
          <div class="col-12">
            <div class="border mt-4 p-2 pt-4">
              <fieldset>
                <legend class="text-uppercase">Select Payment Setting</legend>
                <div class="row">
                  <div class="col-md-6">
                    <div class="form-group">
                      <label for="user">Driver</label>
                      <select class="form-control custom-select">
                        <option>Manual</option>
                        <option>ICVerify</option>
                        <option>ICVerify UK</option>
                      </select>
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <label for="user">Bank Def.</label>
                      <input type="text" class="form-control" placeholder=" ">
                    </div>
                  </div>
                </div>
              </fieldset>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-12">
            <div class="border mt-4 p-2 pt-4">
              <fieldset>
                <legend class="text-uppercase">Order Class (es) Included</legend>
                <div class="row justify-content-between align-items-center ">
                  <div class="col-md-6 ">
                    <div class="d-flex flex-row align-items-center ">
                      <div class="p-2 ">OC Filter :</div>
                      <div class="col-3 p-2">
                        <select class="form-control custom-select">
                          <option>All</option>
                          <option>Subscriptions</option>
                          <option>Subs 'n' Pkgs</option>
                          <option>Product</option>
                        </select>
                      </div>
                      <div class="pt-3 ">
                        <div class="form-check form-check-inline">
                          <label class="custom-control custom-checkbox mr-0">
                            <input type="checkbox" class="custom-control-input">
                              
                            <span class="  custom-control-label">Include All</span>
                          </label>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-md-6 ">
                    <div class="d-flex flex-row-reverse align-items-center ">

                      <div class=" col-3 p-2 ">
                        <select class="form-control custom-select">
                          <option>All</option>
                          <option>Selected</option>
                        </select>

                      </div>
                      <div class="p-2 ">Show :</div>
                    </div>
                    <div class="clearfix "></div>
                  </div>
                </div>
                <div class="agGridSize">
                  <app-ag-grid [name]="'RefundsReports'" [myGridOptions]="gridOptions" (myGridReady)="doOnGridReadyRefundsReports($event)"></app-ag-grid>
                </div>
              </fieldset>
            </div>

          </div>
        </div>
        <div class="row">
          <div class="col-12">
            <div class="border mt-4 p-2 pt-4">
              <fieldset>
                <legend class="text-uppercase">Included manual Payment Type &amp; Currency type</legend>
                <div class="row justify-content-between align-items-center ">

                  <div class="col-md-12 ">
                    <div class="d-flex flex-row-reverse align-items-center ">

                      <div class=" col-2 p-2 ">
                        <select class="form-control custom-select">
                          <option>All Cards</option>
                          <option>Selected</option>
                        </select>

                      </div>
                      <div class="p-2 ">Show :</div>
                    </div>
                    <div class="clearfix "></div>
                  </div>
                </div>
                <div class="agGridSize">
                  <app-ag-grid [name]="'RefundsPaymentsReports'" [myGridOptions]="gridOptions1" (myGridReady)="doOnGridReadyRefundPaymentsReports($event)"></app-ag-grid>
                </div>
              </fieldset>
            </div>
          </div>
          <div class="col-12">
            <div class="border mt-4 p-2 pt-4">
              <fieldset>
                <legend class="text-uppercase">Currencies
                </legend>
                <div class="agGridSize">
                  <app-ag-grid [name]="'RefundsCurrenciesReports'" [myGridOptions]="gridOptions2" (myGridReady)="doOnGridReadyRefundCurrenciesReports($event)"></app-ag-grid>
                </div>
              </fieldset>
            </div>
          </div>
        </div>


      </div>
      <div class="row">
        <div class="col-md-12 text-right my-3">
          <button type="button" class="btn btn-primary" data-dismiss="modal">
            <i class="fa fa-floppy-o"></i> Save
          </button>
          <button type="button" class="btn btn-light" data-dismiss="modal">
            <i class="fa fa-times" aria-hidden="true"></i> Cancel
          </button>
        </div>
      </div>
    </my-tab>
    <my-tab tabTitle="Output" [tabId]="REFUND_OUTPUT_REPORT">
      <app-output></app-output>
    </my-tab>
  </my-tabs>
</section>
