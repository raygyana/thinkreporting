<div class="modal-body">
  <div class="clearfix"></div>
  <div class="content-box-inner">
    <div class="row">
      <div class="form-group col-md-6">
        <label for="LastName">Order Item Price :</label>
      </div>

      <div class="form-group col-md-6">
        <input type="text " class="form-control " placeholder=" " name="orderitemprice" [(ngModel)]="changeAmountModel.orderitemprice">
      </div>
    </div>

    <div class="row">
      <div class="form-group col-md-6">
        <label for="LastName">Delivery Charge :</label>
      </div>

      <div class="form-group col-md-6">
        <input type="text " class="form-control " placeholder=" " name="deliverycharge" [(ngModel)]="changeAmountModel.deliverycharge" readonly="readonly">
      </div>
    </div>

    <br/>
    <div class="row ">
      <div class="col-md-12 text-right my-3">
        <button type="submit" class="btn btn-primary" data-dismiss="modal" (click)="changeorderamount()"> OK</button>

      </div>
    </div>
  </div>
</div>
