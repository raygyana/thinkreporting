<section class="container-fluid my-3 custRow ">
  <div class="row ">
    <div class="col-md-12 ">
      <div class="whiteBg tabContent active_link topArrow topArrowPosition1 ">

        <div class="agGridSize">
          <app-ag-grid [name]="'documentreference'" [myGridOptions]="gridOptions" (myGridReady)="doOnGridReadyDocRef($event)">
          </app-ag-grid>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="form-group col-md-12 text-right ">
      <div class="right popup-border-btn col-md-9 ">
        <button type="submit" class="btn btn-primary" (click)="addNewCustomer()">
          <i class="fa fa-plus " aria-hidden="true "></i> Add New
        </button>
        <button type="submit" [class.disabled]="rowSelected ? false : true" class="btn btn-primary" (click)="useExiting()">
          <i class="fa fa-plus " aria-hidden="true "></i> Edit Exiting

        </button>

      </div>

    </div>
  </div>

</section>
