<div class="tab-content">
  <form #baseForm="ngForm">
    <div id="tab1" class="tab-pane fade show active ">
      <div class="whiteBg tabContent">
        <div class="content-box-inner">
          <div class="row justify-content-between align-items-center">
            <div class="col-md-3">
              <div class="form-group row">
                <label for="historyFilter" class="col-sm-4 pr-0 pl-0 col-form-label">History Filters</label>
                <div class="col-sm-8">
                  <app-select name="historyFiltersData" [Data]="ddOptionsCustHistoryData" [(ngModel)]="customerHistoryModel.historyFilter.value"
                    (change)="changeTableData($event)">
                  </app-select>
                  <!-- <select class="form-control custom-select" name="historyFilter" [(ngModel)]="customerHistoryModel.historyFilter.value" (change)="changeTableData($event)">
                  <option [value]="dropOptn.key" *ngFor="let dropOptn of ddOptionsCustHistoryData">
                    {{dropOptn.value}}
                  </option>
                </select> -->
                </div>
              </div>
            </div>
            <div class="col-md-4 text-right">
            </div>
          </div>
          <hr class="borderBtm">
          <div class="row">
            <div class="col-md-12">
              <div class="agGridSize">
                <app-ag-grid [name]="'AutoRenewalReports'" [myGridOptions]="gridOptionsReports" (myGridReady)="doOnGridReadyAutoRenewalReports($event)"></app-ag-grid>
              </div>
            </div>

          </div>
        </div>
      </div>
    </div>
  </form>
</div>
